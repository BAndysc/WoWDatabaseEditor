          "db_column_name": "max_limit",
          "value_type": "uint"
        },
        {
          "name": "Members",
          "is_meta": true,
          "meta": "table:pool_members;`poolSpawnId` = {entry}"
        },
        {
          "name": "Description",
          "db_column_name": "description",
          "value_type": "string",
