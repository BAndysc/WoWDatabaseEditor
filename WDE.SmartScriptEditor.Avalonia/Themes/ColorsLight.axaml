<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <SolidColorBrush x:Key="SmartScripts.Background" Color="#F5F6FC" />

        <SolidColorBrush x:Key="SmartScripts.Actions.Background" Color="#F5F6FC" />
        <SolidColorBrush x:Key="SmartScripts.Action.Border" Color="#E9EFFB" />
        <SolidColorBrush x:Key="SmartScripts.Action.Background" Color="#E9EFFB" />
        <SolidColorBrush x:Key="SmartScripts.Action.Selected.Border" Color="#F5C61F" />
        <SolidColorBrush x:Key="SmartScripts.Action.Selected.Background" Color="#FFF2BF" />
        <SolidColorBrush x:Key="SmartScripts.Action.Foreground" Color="#282828" />

        <SolidColorBrush x:Key="SmartScripts.Parameter.Foreground" Color="#3C76BE" />
        <SolidColorBrush x:Key="SmartScripts.Source.Foreground" Color="#2E8A8A" /> <!-- #378B6F-->

