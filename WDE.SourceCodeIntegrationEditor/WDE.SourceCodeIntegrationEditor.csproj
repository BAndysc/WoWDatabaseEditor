    </ItemGroup>
    <ItemGroup>
      <PackageReference Include="envdte" Version="17.9.37000" />
    </ItemGroup>
    <PropertyGroup>
        <OutputPath>..\bin\$(Configuration)\</OutputPath>
    </PropertyGroup>
    <Import Project="..\Module.props" />    

</Project>
