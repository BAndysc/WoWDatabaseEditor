<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <Color x:Key="TreeViewItem.Header.Background.SpecialColor">#2D619E</Color>
        <SolidColorBrush x:Key="TreeViewItem.Header.Background.Special" Accent.BaseColor="{StaticResource TreeViewItem.Header.Background.SpecialColor}"  Accent.Hue="{DynamicResource AccentHue}" />
        
        <SolidColorBrush x:Key="TreeViewItem.Header.Foreground.Special" Color="#F2F2F2" />
        <SolidColorBrush x:Key="TreeViewItem.Content.Background.Modified" Color="#023e7d" Opacity=".5"/>
        <SolidColorBrush x:Key="TreeViewItem.Content.Background.Normal" Color="#FF252526"/>
        <SolidColorBrush x:Key="TreeViewItem.Content.Background.NormalAlternate" Color="#3B3B3D"/>
        <SolidColorBrush x:Key="TreeViewItem.Content.Background.Selected" Color="#465058"/>
        
        <SolidColorBrush x:Key="SlightlyDifferentBackground" Color="#242424" />
    </Styles.Resources>
</Styles>
