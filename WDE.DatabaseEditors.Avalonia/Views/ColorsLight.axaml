<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <Color x:Key="TreeViewItem.Header.Background.SpecialColor">#4E88BF</Color>
        <SolidColorBrush x:Key="TreeViewItem.Header.Background.Special" Accent.BaseColor="{StaticResource TreeViewItem.Header.Background.SpecialColor}"  Accent.Hue="{DynamicResource AccentHue}" />
        <SolidColorBrush x:Key="TreeViewItem.Header.Foreground.Special" Color="#F1F1F1" />
        <SolidColorBrush x:Key="TreeViewItem.Content.Background.Modified" Color="#000099" Opacity=".3"/>
        <SolidColorBrush x:Key="TreeViewItem.Content.Background.Normal" Color="#FFFFFFFF"/>
        <SolidColorBrush x:Key="TreeViewItem.Content.Background.NormalAlternate" Color="#F0F0F0"/>
        <SolidColorBrush x:Key="TreeViewItem.Content.Background.Selected" Color="LightBlue"/>

        <SolidColorBrush x:Key="SlightlyDifferentBackground" Color="#F7F7F7" />
    </Styles.Resources>
</Styles>
