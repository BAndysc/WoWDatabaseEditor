<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:managers="clr-namespace:WoWDatabaseEditorCore.Avalonia.Managers"
             xmlns:utils="clr-namespace:WDE.Common.Avalonia.Utils;assembly=WDE.Common.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="managers:WindowViewModelDocumentWrapper"
             x:Class="WoWDatabaseEditorCore.Avalonia.Views.WindowToolbarDocumentWrapper">
    <ContentControl DataContext="{CompiledBinding Document}" Content="{CompiledBinding}" ContentTemplate="{x:Static utils:ToolbarDataTemplate.Template}" />
</UserControl>
