<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WoWDatabaseEditorCore.Avalonia.Services.InputEntryProviderService.InputEntryProviderView">
    <StackPanel>
        <Label Content="Enter entry:" HorizontalAlignment="Center"
               Margin="0, 10, 0, 10"/>
        <TextBox Text="{Binding Entry}" />
        <Button Content="Ok" Command="{Binding Save}"
                HorizontalAlignment="Center"
                Margin="0, 10, 0, 10"/>
    </StackPanel>
</UserControl>
